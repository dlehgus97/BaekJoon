#파이썬
a,b=map(int,input().split())
if a>b:
    print(">")
elif a<b:
    print("<")
elif a==b:
    print("==")

//c++
#include<iostream>
using namespace std;

int main() {
	int a; int b; 
	cin >> a >> b;
	if (a > b)
		cout << ">" << endl;
	else if (a < b)
		cout << "<" << endl;
	else
		cout << "==" << endl;
	return 0;

}
