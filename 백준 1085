#파이썬
x,y,w,h = map(int,input().split())
print(min(x,y,h-y,w-x))

//c++
#include <iostream>
#include <algorithm>  

using namespace std;

int main(){
	int x, y, w, h;
	int result1,result2;
	cin >> x >> y >> w >> h;
	result1 = min(x, y);
	result2 = min(w - x, h - y);
	cout << min(result1, result2);
	

}
