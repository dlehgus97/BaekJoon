#파이썬 
a = int(input())
b = int(input())
print(a*(b%10))   #주어진수에서 10을 나누어 나온 나머지는 1의 자리수이다 .
print(a*int((b%100 -b%10)/10)) #백을 나눠 나오는 두자리수의 몫에 10을 나눠 나오는 한자리수 몫을 뺴면 10의 단위로 끊기면서 10을 나눠주면 두번째 자리가 나온다. int 를 붙여 정수로 바꾸어준다
print(a*(b // 100)) #세자수이기떄문에 100을 나눴을떄 나오는 몫이 첫번째자리수 이다.
print(a*b)

#파이썬 
a = int(input())
b= str(input())
 
print(a * int(b[2]))
print(a * int(b[1]))
print(a * int(b[0]))
print(a * int(b))
#두번쨰 방법이다. b를 str 취급하여 하나하나식 인식받아 곱해준다.

//c++
#include <iostream>

using namespace std;

int main() {
	int a; int b;
	cin >> a >> b;
	cout << a * (b % 10) << "\n";
	cout << a * (b % 100 - b % 10) / 10 << "\n";
	cout << a * (b / 100) << "\n";
	cout << a * b << "\n";

}
//역시 나머지를 이용해서 구해주었다. 
