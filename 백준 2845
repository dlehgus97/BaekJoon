#파이썬
a,b =map(int,input().split())
k = map(int,input().split())
result = a*b
for i in k:
    print(i - result, end = ' ')   #end = '' 는 끝나고 출력값이 \n처리되지 않기 위해 
    
//c++
#include<iostream>

using namespace std;

int main() {
	int a; int b; int c;
	cin >> a >> b;
	int result = a * b;
	for (int i = 0; i < 5; i++) {
		cin >> c;
		cout << c - result << " ";
	}
}
