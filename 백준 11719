#파이썬 1
import sys
for i in sys.stdin:
    print(i , end = '')  #속도로 인해 import 해서 사용
   
#파이썬 2 
while True:
    try:
        print(input())
    except EOFError:      #보기 쉽게 except EOFError 을 사용했지만 except 만 사용해도 된다.
        break             
        
 //c++
 #include<iostream>
#include<string>

using namespace std;

int main() {
	string s;
	for (int i = 0; i < 100; i++) {
		getline(cin, s);
		cout << s << endl;
	}

}

include <iostream>
#include <string>
using namespace std;
int main() {
    string a;
    while (getline(cin, a)) {
        cout << a << endl;
    }
}
/100개의 글자 제한이 있기때문에 for문 사용하여 조건을 붙여주었다. 
