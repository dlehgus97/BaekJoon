#파이썬
k = int(input())
for i in range (1,k+1):
    a,b =map(int,input().split())
    print("Case #%s: %s"%(i,a+b))
    
//c++
#include<iostream>

using namespace std;

int main() {
	int a; int b; int c;
	cin >> a;
	for (int i = 1; i <= a; i++) {
		cin >> b >> c;
		cout << "Case #" << i << ": " << b + c << "\n";
	}
}
//앞문제랑 똑같음 영양가 없는 문제 
