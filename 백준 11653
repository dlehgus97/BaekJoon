#파이썬
n = int(input())

while(n!= 1):
    for i in range (2,n+1):
        if(n%i== 0):
            print(i)
            n =n//i
            break
            
//c++
#include <iostream>

using namespace std;

int main(){
	int n;
	cin >> n;
	while (n != 1) {
		for (int i = 2; i <= n; i++) {
			if (n % i == 0) {
				cout << i<<"\n";
				n /= i;
				break;
			}
		}
	}
}
