#파이썬
print("     /~\\")
print("    ( oo|")
print("    _\\=/_")
print("   /  _  \\")
print("  //|/.\\|\\\\")
print(" ||  \\ /  ||")
print("============")
print("|          |")
print("|          |")
print("|          |")

//c++
#include <iostream>
using namespace std;
int main()
{
	printf("     /~\\\n");
	printf("    ( oo|\n");
	printf("    _\\=/_\n");
	printf("   /  _  \\\n");
	printf("  //|/.\\|\\\\\n");
	printf(" ||  \\ /  ||\n");
	printf("============\n");
	printf("|          |\n");
	printf("|          |\n");
	printf("|          |\n");
}
