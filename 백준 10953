#파이썬
k = int(input())
for i in range (k):
    a,b, =map(int,input().split(","))   #split()이 구분을 위한것이기에 " , " 하나를 추가해서 구분해준다
    print(a+b)

//c++
#include<iostream>

using namespace std;

int main() {
	int a; int b; int c; char d;
	cin >> a;
	for (int i = 0; i < a; i++) {
		cin >> b >> d >> c;
		cout << b + c << endl;
	}
}
//char d를 추가해서 ,를 cin 해주면 된다
