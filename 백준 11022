#파이썬
k = int(input())
for i in range (1,k+1):
    a,b =map(int,input().split())
    print("Case #%s: %s + %s = %s"%(i,a,b,a+b))

//c++
#include<iostream>

using namespace std;

int main() {
	int a; int b; int c;
	cin >> a;
	for (int i = 1; i <= a; i++) {
		cin >> b >> c;
		cout << "Case #" << i << ": " << b << " + " << c << " = " << b + c << "\n";
	}
}
