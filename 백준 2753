#파이썬

a=int(input())
if a % 400 == 0:
    print(1)
elif a % 4 ==0 and a % 100 !=0:
    print(1)
elif a % 4 !=0 and a % 100 == 0:
    print(0)
else :
    print(0)
    
//c++

#include<iostream>

using namespace std;

int main() {
	int a; 
	cin >> a;
	if (a % 400 == 0)
		printf("1");
	else if (a % 4 == 0 and a % 100 != 0)
		printf("1");
	else if (a % 4 != 0 and a % 100 == 0)
		printf("0");
	else
		printf("0");
    
//a!= b a와b가 다르다 . a==0 a와b가 같다 
