#파이썬
for i in range (3):
    result = list(map(int,input().split()))
    if result.count(1) == 0:
        print("D")
    if result.count(1) == 1:
        print("C")
    if result.count(1) == 2:
        print("B")
    if result.count(1) == 3:
        print("A")
    if result.count(1) == 4:
        print("E")
    
 //c++
 #include<iostream>

using namespace std;

int main() {
	for (int i = 0; i < 3; i++) {
		int a, b, c, d;
		cin >> a >> b >> c >> d;
		int sum = a + b + c + d;
		if (sum == 0)
			cout << 'D'<<  "\n";
		if (sum == 1)
			cout << 'C' << "\n";
		if (sum == 2)
			cout << 'B' << "\n";
		if (sum == 3)
			cout << 'A' << "\n";
		if (sum == 4)
			cout << 'E' << "\n";

	}
}
