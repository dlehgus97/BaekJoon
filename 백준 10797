#파이썬
a= int(input())
b = 0
c = list(map(int,input().split()))
for i in (c):
    if a == i:
        b += 1
print(b)

//c++
#include <iostream>


using namespace std;

int main() {
	int a,b;
	int count = 0;
	cin >> a;
	for (int i = 0; i < 5; i++) {
		cin >>  b ;
		if (a == b) {
			count += 1;
		}
	}
	cout << count;
}
